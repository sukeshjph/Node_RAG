    "@langchain/openai": "^0.6.11",
    "dotenv": "^16.4.5",
    "js-tiktoken": "^1.0.21",
    "langchain": "^0.3.33",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.1"
