{
  "name": "azureaiproject",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/create-index.js",
    "create-index": "node --loader ts-node/esm src/createorUpdateIndex/create-index.ts",
    "create-index-rest": "node --loader ts-node/esm src/createorUpdateIndex/create-index-rest.ts",
    "test-index": "node --loader ts-node/esm src/test/test-index.ts",
    "test-embedding": "node --loader ts-node/esm src/test/embed.ts",
